{
  "name": "@smockle/safe",
  "version": "2.0.0",
  "description": "Verifies argument types.",
  "main": "lib/safe.js",
  "scripts": {
    "docs": "jsdoc2md \"lib/**/*.js\" --template jsdoc2md/README.hbs --example-lang JavaScript > README.md",
    "test": "tape \"test/**/*.js\" | colortape",
    "coverage": "nyc --reporter=lcov tape \"test/**/*.js\" | colortape",
    "postcoverage": "nyc report",
    "codeclimate": "codeclimate-test-reporter < ./coverage/lcov.info",
    "prepublish": "npm test",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/safe.git"
  },
  "author": "Clay Miller <clay@smockle.com> (https://smockle.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smockle/safe/issues"
  },
  "homepage": "https://github.com/smockle/safe#readme",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.0",
    "colortape": "^0.1.2",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.0",
    "husky": "^0.13.2",
    "jsdoc-to-markdown": "^3.0.0",
    "lint-staged": "^3.4.0",
    "nyc": "^11.0.2",
    "prettier-eslint-cli": "^4.0.0",
    "tape": "^4.5.1"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier-eslint --write",
      "git add"
    ]
  }
}
